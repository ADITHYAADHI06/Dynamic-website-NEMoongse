<div class="conatiner-fluid mt-5 about-style" id="contactid">
       <div class="row">
           <div class="col-md-10 hero-text col-12 mx-auto">
               <h1 class="text-center main-heading">Contact us</h1>
               <p class="text-center side-heading">I'm happy to help you.</p>
             <div class="row d-flex justify-content-center">
<div class="col-md-6 col-10 ">

  <form action="/contact" method="POST">
 
  <div class="form-group mb-3">
    <label for="exampleInputName1" class="form-label">Full name</label>
    <input type="text" class="form-control" id="exampleInputName1" placeholder="Enter Your Name" name="name">
  </div>
 <div class ="form-group mb-3">
    <label for="exampleInputEmail1" class="form-label">Email address</label>
    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter Your Email" name="email">
  </div>
   <div class ="form-group mb-3">
    <label for="exampleInputPhonenumber1" class="form-label">PhoneNumber</label>
    <input type="number" class="form-control" id="exampleInputPhonenumber1" aria-describedby="numberHelp" placeholder="Phonenumber" name="phone">
  </div>
   <div class ="form-group mb-3">
    <label for="exampleInputMsg1" class="form-label">Message</label>
    <input type="text" class="form-control" id="exampleInputMsg1" aria-describedby="emailHelp" placeholder="Message" name="message">

  </div>

  <button type="submit" class="btn btn-style btn-primary">Submit</button>
</form>
</div>

             </div>
           </div>
       </div>
</div>
